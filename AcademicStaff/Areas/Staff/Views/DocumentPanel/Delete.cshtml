
@model AcademicStaff.Models.Entities.Document


@{
    ViewBag.Title = "Delete Document";
    Layout = "~/Views/Shared/_AreaLayout.cshtml";
}
<div class="row">
    <div class="col-md-3 col-xs-1 col-sm-2"></div>
    <div class="col-md-6 col-xs-10 col-sm-8">
        <div class="box box-default">
            <div class="box-header with-border">
                <i class="fa fa-warning"></i>

                <h3 class="box-title">Delete Document</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <div class="alert alert-danger alert-dismissible">

                    <h4>
                        <i class="icon fa fa-ban"></i> Alert! Are you sure you want to delete this?
                    </h4>
                    <dl class="dl-horizontal">
                        <dt>
                            DOCUMENT NAME
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.DocName)
                        </dd>
                    </dl>
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-actions no-color btn-delete">
                            <input type="submit" value="Delete" class="btn btn-default" /> &nbsp;
                            <a href="javascript: history.back()" class="btn btn-primary">Cancel</a>

                        </div>
                    }


                </div>

            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
    <!-- /.col -->


    <div class="col-md-3 col-xs-2 col-sm-2"></div>

</div>



